module packages

language GoLang
start symbol TestSources

test package in multiple files [[
  ["test"]
  package test;
  
  type [[Point]] struct{
      X, Y int;
  };

  ["test"]
  package test;

  var p [[Point]];
]] resolve #2 to #1

test import with default name [[
  ["."]
  package main;

  import (
      "test";
  );

  var p test.[[Point]];

  ["test"]
  package test;

  type [[Point]] struct {
      X, Y int;
  };
]] resolve #1 to #2

test import with custom name [[
  ["."]
  package main;

  import (
      t "test";
  );

  var p t.[[Point]];

  ["test"]
  package test;

  type [[Point]] struct {
      X, Y int;
  };
]] resolve #1 to #2

