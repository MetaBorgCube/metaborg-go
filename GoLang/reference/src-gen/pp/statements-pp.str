  prettyprint-minijava-Statement :
    Print(t1__) -> [ H(
                       [SOpt(HS(), "0")]
                     , [ S("System.out.println(")
                       , t1__'
                       , S(");")
                       ]
                     )
                   ]
    with t1__' := <pp-one-Z(prettyprint-minijava-Exp) <+ pp-one-Z(prettyprint-completion-aux)> t1__

  is-minijava-Statement =
    ?Print(_)

  minijava-get-indent :
    (t@Print(t1__), index) -> indent
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
