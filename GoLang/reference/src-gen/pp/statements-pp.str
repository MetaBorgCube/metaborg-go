                              , [ S("if(")
                                , t1__'
                                , S(") ")
                                , t2__'
                                ]
                              )
                            , H(
                                [SOpt(HS(), "0")]
                              , [S("else "), t3__']
                              )
                            ]
    with t1__' := <pp-one-Z(prettyprint-minijava-Exp) <+ pp-one-Z(prettyprint-completion-aux)> t1__
    with t2__' := <pp-one-Z(prettyprint-minijava-Statement) <+ pp-one-Z(prettyprint-completion-aux)> t2__
    with t3__' := <pp-one-Z(prettyprint-minijava-Statement) <+ pp-one-Z(prettyprint-completion-aux)> t3__

