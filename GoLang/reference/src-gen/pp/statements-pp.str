         end

  prettyprint-minijava-Statement :
    While(t1__, t2__) -> [ H(
                             [SOpt(HS(), "0")]
                           , [ S("while(")
                             , t1__'
                             , S(") ")
                             , t2__'
                             ]
                           )
                         ]
