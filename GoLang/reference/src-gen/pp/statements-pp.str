         end

  minijava-get-separator :
    (t@Print(t1__), index) -> separator
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             separator := ""
           otherwise :
             separator := ""
         end

  minijava-get-leading-ws :
    (t@Print(t1__), index) -> ws
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             ws := ""
           otherwise :
             ws := ""
         end

  is-minijava-Statement =
    fail

  prettyprint-minijava-Statement :
    amb([h|hs]) -> <prettyprint-minijava-Statement> h

  prettyprint-minijava-VarRef :
    VarRef-Plhdr() -> [ H(
                          [SOpt(HS(), "0")]
                        , [S("[[VarRef]]")]
                        )
                      ]

  is-minijava-VarRef =
    ?VarRef-Plhdr()

  prettyprint-minijava-Statement :
    Statement-Plhdr() -> [ H(
                             [SOpt(HS(), "0")]
                           , [S("[[Statement]]")]
                           )
                         ]

  is-minijava-Statement =
