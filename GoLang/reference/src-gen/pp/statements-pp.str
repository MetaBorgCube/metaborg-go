module reference/src-gen/pp/statements-pp

imports
  libstratego-gpp
  libspoofax/sdf/pp
  libspoofax/term/origin
  libstratego-sglr
  reference/src-gen/signatures/statements-sig


imports
  reference/src-gen/signatures/expressions-sig


imports
  reference/src-gen/pp/expressions-pp


strategies
  prettyprint-example =
    prettyprint-minijava

  prettyprint-minijava =
    fail


strategies
  prettyprint-SDF-start-symbols =
    prettyprint-minijava-start-symbols

  prettyprint-minijava-start-symbols =
    fail


strategies
  prettyprint-minijava =
    prettyprint-minijava-Statement

  prettyprint-minijava(|sort):
    t -> <prettyprint-minijava-Statement>
    where <?"Statement"> sort

  prettyprint-minijava-Statement :
    Block(t1__) -> [ H(
                       [SOpt(HS(), "0")]
                     , [S("{")]
                     )
                   , t1__'
                   , H(
                       [SOpt(HS(), "0")]
                     , [S("}")]
                     )
                   ]
    with t1__' := <pp-indent(|"2")> [ <pp-V-list(prettyprint-minijava-Statement) <+ pp-one-Z(prettyprint-completion-aux)> t1__
                                    ]

  is-minijava-Statement =
    ?Block(_)

  minijava-get-indent :
    (t@Block(t1__), index) -> indent
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             indent := "  "
           otherwise :
             indent := ""
         end

  minijava-get-separator :
    (t@Block(t1__), index) -> separator
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             separator := "\n"
           otherwise :
             separator := ""
         end

  minijava-get-leading-ws :
    (t@Block(t1__), index) -> ws
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             ws := "\n  "
           otherwise :
             ws := ""
         end

  prettyprint-minijava-Statement :
    Assign(t1__, t2__) -> [ H(
                              [SOpt(HS(), "0")]
                            , [ t1__'
                              , S(" = ")
                              , t2__'
                              , S(";")
                              ]
                            )
                          ]
    with t1__' := <pp-one-Z(prettyprint-minijava-VarRef) <+ pp-one-Z(prettyprint-completion-aux)> t1__
    with t2__' := <pp-one-Z(prettyprint-minijava-Exp) <+ pp-one-Z(prettyprint-completion-aux)> t2__

  is-minijava-Statement =
    ?Assign(_, _)

  minijava-get-indent :
    (t@Assign(t1__, t2__), index) -> indent
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             indent := ""
           case ?2 :
             indent := ""
           otherwise :
             indent := ""
         end

  minijava-get-separator :
    (t@Assign(t1__, t2__), index) -> separator
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             separator := ""
           case ?2 :
             separator := ""
           otherwise :
             separator := ""
         end

  minijava-get-leading-ws :
    (t@Assign(t1__, t2__), index) -> ws
    where sort := <ia-get-sort> t
          ; <?"Statement"> sort
    with switch !index
           case ?1 :
             ws := ""
           case ?2 :
             ws := " "
           otherwise :
             ws := ""
         end

  prettyprint-minijava-Statement :
    ArrayAssign(t1__, t2__, t3__) -> [ H(
                                         [SOpt(HS(), "0")]
                                       , [ t1__'
                                         , S("[")
                                         , t2__'
                                         , S("] = ")
                                         , t3__'
                                         , S(";")
                                         ]
                                       )
                                     ]
    with t1__' := <pp-one-Z(prettyprint-minijava-VarRef) <+ pp-one-Z(prettyprint-completion-aux)> t1__
    with t2__' := <pp-one-Z(prettyprint-minijava-Exp) <+ pp-one-Z(prettyprint-completion-aux)> t2__
    with t3__' := <pp-one-Z(prettyprint-minijava-Exp) <+ pp-one-Z(prettyprint-completion-aux)> t3__

  is-minijava-Statement =
    ?ArrayAssign(_, _, _)

  minijava-get-indent :
    ( t@ArrayAssign(t1__, t2__, t3__)
    , index
    ) -> indent
    where sort := <ia-get-sort> t
